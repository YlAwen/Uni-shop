.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
.sugg-list {
  padding: 0 5px;
}
.sugg-list .sugg-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 12px;
  padding: 13px 0;
  border-bottom: 1px solid #efefef;
}
.sugg-list .sugg-item .goods-name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.histroy-box {
  padding: 0 5px;
}
.histroy-box .histroy-title {
  display: flex;
  justify-content: space-between;
  height: 40px;
  align-items: center;
  font-size: 13px;
  border-bottom: 1px solid #efefef;
}
.histroy-box .histroy-list {
  display: flex;
  flex-wrap: wrap;
}
.histroy-box .histroy-list .van-tag {
  margin: 5px 5px 0 0;
}

